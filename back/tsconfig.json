{
    "compilerOptions": {
      "types": ["node", "express"],
      "target": "ES2021",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "strict": true,
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "useDefineForClassFields": false,
      "esModuleInterop": true,
      "strictPropertyInitialization": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist",".next", "**/AppData", "**/Documents"]
  }
  